// anonymous function expression
foo = function(msg): print(msg); end;
foo("foo");

// explicit identifier
function bar(msg):
	print(msg);
end;

x = bar("bar");
print(x); // null due to void return

// function parameter
function foobar(func, arg):
	func(arg);
end;

foobar(function(msg): print(msg); end, "test");